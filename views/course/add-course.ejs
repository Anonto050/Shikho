<!doctype html>
<html class="no-js" lang="zxx">
<%- include head.ejs %>
<body>

<!-- Add your site or application content here -->

<!-- pre loader area start -->
<%- include preloader.ejs %>
<!-- pre loader area end -->

<!-- back to top start -->
<%- include back_to_top.ejs %>
<!-- back to top end -->

<!-- header area start -->
<%- include navigation_bar.ejs %>

<main>

    <% if(create_button=='false' ) { %>
    <!-- sign up area start -->
    <section class="signup__area po-rel-z1 pt-100 pb-145">
        <div class="sign__shape">
            <img class="man-1" src="/images/icon/sign/man-3.png" alt="">
            <img class="man-2 man-22" src="/images/icon/sign/man-2.png" alt="">
            <img class="circle" src="/images/icon/sign/circle.png" alt="">
            <img class="zigzag" src="/images/icon/sign/zigzag.png" alt="">
            <img class="dot" src="/images/icon/sign/dot.png" alt="">
            <img class="bg" src="/images/icon/sign/sign-up.png" alt="">
            <img class="flower" src="/images/icon/sign/flower.png" alt="">
        </div>
        <div class="container">
            <div class="row">
                <div class="col-xxl-8 offset-xxl-2 col-xl-8 offset-xl-2">
                    <div class="section__title-wrapper text-center mb-55">
                        <h2 class="section__title">Create a New Course</h2>

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xxl-6 offset-xxl-3 col-xl-6 offset-xl-3 col-lg-8 offset-lg-2">
                    <div class="sign__wrapper white-bg">

                        <div class="sign__form">
                            <form method="post" action="/instructor/user/<%= userInfo[0] %>/add-course/pre" enctype="multipart/form-data">

                                <div class="sign__input-wrapper mb-25">
                                    <h5>TITLE</h5>
                                    <div class="sign__input">
                                        <input type="text" name="course_name" id="course_name" placeholder="Course Name"/>
                                        <i class="fal fa-user"></i>
                                    </div>
                                </div>
                                <div class="sign__input-wrapper mb-25">
                                    <h5>DESCRIPTION</h5>
                                    <div class="sign__input">
                                        <input type="text" name="description" id="description" placeholder="Course Description"/>
                                        <i class="fal fa-envelope"></i>
                                    </div>
                                </div>

                                <div class="sign__input-wrapper mb-25">
                                    <h5>EDUCATIONAL LEVEL</h5>
                                    <div class="sign__input">
                                        <input type="text" name="level" id="level" placeholder="Course Level"/>
                                        <i class="fal fa-user"></i>
                                    </div>
                                </div>
                                <div class="sign__input-wrapper mb-25">
                                    <h5>CATEGORY</h5>
                                    <div class="sign__input">
                                        <input type="text" name="catagory" id="catagory" placeholder="Which category does this course belong to?"/>
                                        <i class="fal fa-lock"></i>
                                    </div>
                                </div>

                                <div class="sign__input-wrapper mb-10">
                                    <h5>Course Picture</h5>
                                    <div class="sign__input">
                                        <input class="form-control" runat="Server" type="file" name="uploaded_image" enctype="multipart/form-data" accept=""/>
                                        <i class="fas fa-camera"></i>
                                    </div>
                                </div>

                                <button class="e-btn w-100"> <span></span> Create New </button>

                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

        <% } else { %>
            <section class="page__title-area pt-120 pb-90">
                <div class="page__title-shape">
                    <img class="page-title-shape-5 d-none d-sm-block" src="/images/page-title/page-title-shape-1.png" alt="">
                    <img class="page-title-shape-6" src="/images/page-title/page-title-shape-6.png" alt="">
                    <img class="page-title-shape-7" src="/images/page-title/page-title-shape-4.png" alt="">
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-xxl-8 col-xl-8 col-lg-8">
                            <div class="course__wrapper">
                                <div class="page__title-content mb-25">
                                    <div class="page__title-breadcrumb">
                                        <nav aria-label="breadcrumb">
                                            <ol class="breadcrumb">
                                                <% if( isStudent=='true' ) { %>
                                                    <li class="breadcrumb-item"><a href="/student/user/<%= userInfo[0] %>/">Home</a></li>
                                                    <li class="breadcrumb-item"><a href="/student/user/<%= userInfo[0] %>/courses">Courses</a></li>
                                                <% } else { %>
                                                    <li class="breadcrumb-item"><a href="/instructor/user/<%= userInfo[0] %>/">Home</a></li>
                                                    <li class="breadcrumb-item"><a href="/instructor/user/<%= userInfo[0] %>/courses">Courses</a></li>
                                                <% } %>

                                                <li class="breadcrumb-item active" aria-current="page"><%=course[9] %></li>
                                            </ol>
                                        </nav>
                                    </div>
                                    <span class="page__title-pre"><%=course[14] %></span>
                                    <h5 class="page__title-3"><%=course[9] %></h5>
                                </div>
                                <div class="course__meta-2 d-sm-flex mb-30">
                                    <div class="course__teacher-3 d-flex align-items-center mr-70 mb-30">
                                        <div class="course__teacher-thumb-3 mr-15">
                                            <img src="/images/course/teacher/teacher-1.jpg" alt="">
                                        </div>
                                        <div class="course__teacher-info-3">
                                            <h5>Teacher</h5>
                                            <p><a href="#"><%=teachers[0][0] %></a></p>
                                        </div>
                                    </div>

                                    <div class="course__rating-2 mb-30">
                                        <h5>Review:</h5>
                                        <div class="course__rating-inner d-flex align-items-center">
                                            <ul>
                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                            </ul>
                                            <p><%=course[15] %></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="course__tab-2 mb-45">
                                    <ul class="nav nav-tabs" id="courseTab" role="tablist">
                                        <li class="nav-item" role="presentation">
                                            <button class="nav-link active" id="description-tab" data-bs-toggle="tab" data-bs-target="#description" type="button" role="tab" aria-controls="description" aria-selected="true"> <i class="icon_ribbon_alt"></i> <span>Description</span> </button>
                                        </li>
                                        <li class="nav-item" role="presentation">
                                            <button class="nav-link " id="curriculum-tab" data-bs-toggle="tab" data-bs-target="#curriculum" type="button" role="tab" aria-controls="curriculum" aria-selected="false"> <i class="icon_book_alt"></i> <span>Curriculum</span> </button>
                                        </li>
                                        <li class="nav-item" role="presentation">
                                            <button class="nav-link" id="review-tab" data-bs-toggle="tab" data-bs-target="#review" type="button" role="tab" aria-controls="review" aria-selected="false"> <i class="icon_star_alt"></i> <span>Reviews</span> </button>
                                        </li>
                                        <li class="nav-item" role="presentation">
                                            <button class="nav-link" id="member-tab" data-bs-toggle="tab" data-bs-target="#member" type="button" role="tab" aria-controls="member" aria-selected="false"> <i class="fal fa-user"></i> <span>Members</span> </button>
                                        </li>
                                    </ul>
                                </div>
                                <div class="course__tab-content mb-95">
                                    <div class="tab-content" id="courseTabContent">
                                        <div class="tab-pane fade show active" id="description" role="tabpanel" aria-labelledby="description-tab">
                                            <div class="course__description">
                                                <h3>Course Overview</h3>
                                                <p><%=course[10] %></p>

                                                <div class="course__description-list mb-45">
                                                    <h4>What is the Target Audience?</h4>
                                                    <ul>
                                                        <li> <i class="icon_check"></i> Business's managers, leaders</li>
                                                        <li> <i class="icon_check"></i> Downloadable lectures, code and design assets for all projects</li>
                                                        <li> <i class="icon_check"></i> Anyone who is finding a chance to get the promotion</li>
                                                    </ul>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="tab-pane fade" id="curriculum" role="tabpanel" aria-labelledby="curriculum-tab">
                                            <div class="course__curriculum">

                                                <%for(let topic of topics){%>

                                                <div class="accordion" id="course__accordion">
                                                    <div class="accordion-item mb-50">
                                                        <h2 class="accordion-header" id="week-01">
                                                            <a href="/instructor/user/<%=userInfo[0]%>/add-course/<%=course[0]%>/<%=topic[0]%>" class="link">
                                                            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#week-01-content" aria-expanded="true" aria-controls="week-01-content">
                                                                Topic <%=topic[4]%> : <%=topic[1]%>
                                                            </button>
                                                            </a>
                                                        </h2>
                                                    </div>
                                                </div>
                                                <% } %>
                                                <!--<button class="e-btn w-100"> <span></span> Create New </button>-->
                                                <h3>
                                                <a href="/instructor/user/<%=userInfo[0]%>/add-course/<%=course[0]%>/newModule" class="e-btn">
                                                        <i class="fa fa-plus-circle"></i> New
                                                </a></h3>
                                                <br>
                                                <br>
                                                <%if(newTopic=='true'){%>
                                                    <form method="POST" action="/instructor/user/<%= userInfo[0]%>/add-course/<%=course[0]%>/newModule" enctype="multipart/form-data">

                                                        <tr>
                                                            <td>
                                                                <div class="form-group">
                                                                    <label>Topic Title</label>

                                                                    <input type="text" name="title" id="title" class="form-control" placeholder="Add a Title for this Content">
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <div class="form-group">
                                                                    <label>Description</label>

                                                                    <input type="text" name="description" id="description" class="form-control" placeholder="What's the duration for this topic?">
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <button type="submit" class="btn btn-secondary"><i class="fa fa-check">Submit</i></button>
                                                            </td>
                                                        </tr>
                                                    </form>


                                                <%}else{%>

                                                <%if(weekView =='true'){%>
                                                <div class="create-new others pull-right" id="week1">
                                                    <div class="info">
                                                        <h2>Topic
                                                            <%=thisModule[4]%> : <%=thisModule[1]%>
                                                        </h2>
                                                        <label style="margin-bottom: 0;"><h5>Description : </h5></label>

                                                        <form>
                                                            <input placeholder="TOPIC <%=thisModule[4]%>" name="week_description" id="week_description" type="text" style="width: 60%; margin-top: 0;">
                                                            <button type="submit" class="e-btn">Save</button>
                                                        </form>

                                                    </div>


                                                    <!-- week lesson-->
                                                    <div class="course-table ">

                                                        <table class="table ">
                                                            <thead>

                                                            <tr>
                                                                <th>Type</th>
                                                                <th>Content Title</th>
                                                                <th>Duration</th>
                                                                <th>Status</th>
                                                            </tr>
                                                            </thead>
                                                            <tbody>


                                                            <% for( let index=0; index < VideoContents.length; index++ ) { %>
                                                            <tr style="background-color:rgb(196, 228, 196);">
                                                                <td><i class="fa fa-play-circle"></i></td>
                                                                <td>
                                                                    <a href="">
                                                                        <%=VideoContents[index][2]%>
                                                                    </a>
                                                                </td>
                                                                <td>
                                                                    <%=VideoContents[index][3]%>
                                                                </td>
                                                                <td><i class="fa fa-check "></i></td>
                                                            </tr>
                                                            <%if(QuizContent[index]!=undefined){%>
                                                            <tr style="background-color:rgb(196, 228, 196);">
                                                                <td>
                                                                    <i class="fa fa-question-circle"></i>
                                                                </td>
                                                                <td>
                                                                    <a href=""> Quiz on this topic</a></td>
                                                                <td>31 Min</td>
                                                                <td><i class="fa fa-check"></i></td>
                                                            </tr>
                                                            <%}%>
                                                            <%}%>


                                                            <% if(add_quiz_button == 'false' && add_video_button == 'false') { %>
                                                                <tr>
                                                                    <td>
                                                                        <a href="/instructor/user/<%= userInfo[0]%>/add-course/<%=course[0]%>/<%=thisModule[0]%>/add-video" class="e-btn">Video</a>

                                                                    </td>
                                                                    <td></td>
                                                                    <td></td>
                                                                    <td>
                                                                      <a href="/instructor/user/<%= userInfo[0]%>/add-course/<%=course[0]%>/<%=thisModule[0]%>/add-quiz" class="e-btn">Quiz</a>
                                                                    </td>
                                                                </tr>
                                                            <%  } else{ %>

                                                                <% if(add_video_button == 'true'){%>


                                                                    <form method="POST" action="/instructor/user/<%= userInfo[0]%>/add-course/<%=course[0]%>/<%=thisModule[0]%>/add-video" enctype="multipart/form-data">

                                                                        <tr>
                                                                            <td>
                                                                                <div class="form-group">
                                                                                    <label>Content Title</label>

                                                                                    <input type="text" name="title" id="title" class="form-control" placeholder="Add a Title for this Content">
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>
                                                                                <div class="form-group">
                                                                                    <label>Duartion</label>

                                                                                    <input type="text" name="duration" id="duration" class="form-control" placeholder="What's the duration for this topic?">
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td>
                                                                                <div class="form-group">
                                                                                    <label>Total Marks</label>

                                                                                    <input type="text" name="marks" id="marks" class="form-control" placeholder="Total marks for completing content">
                                                                                </div>
                                                                            </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td><i class="fa fa-play-circle"></i></td>
                                                                            <td>
                                                                                <input type="file" runat="Server" name="uploaded_video" enctype="multipart/form-data" accept="">
                                                                            </td>
                                                                            <td>
                                                                                <button type="submit" class="btn btn-secondary"><i class="fa fa-check"></i></button>
                                                                            </td>
                                                                    </form>

                                                                    </tr>

                                                                <% } else if(add_quiz_button == 'true') {%>

                                                                    <tr>
                                                                        <td><i class="fa fa-question-circle"></i></td>
                                                                        <form method="POST" action="/instructor/user/<%= userInfo[0]%>/add-course/<%=course[0]%>/<%=thisModule[0]%>/add-quiz" enctype="multipart/form-data">

                                                                            <td>Exam</td>
                                                                            <td>
                                                                                <input type="file" name="quizFile" runat="Server">

                                                                            </td>
                                                                            <td>
                                                                                <button type="submit" class="btn btn-secondary"><i class="fa fa-check"></i></button>
                                                                            </td>
                                                                        </form>

                                                                    </tr>

                                                                <% } else if(add_quiz_button == 'true') {%>

                                                                    <tr>
                                                                        <td><i class="fa fa-question-circle"></i></td>
                                                                        <form method="POST" action="/instructor/user/<%= userInfo[0]%>/add-course/<%=course[0]%>/<%=thisModule[0]%>/add-quiz">
                                                                            <td>
                                                                                <input type="file">
                                                                            </td>
                                                                            <td>
                                                                                <button type="submit" class="btn btn-secondary"><i class="fa fa-check"></i></button>
                                                                            </td>
                                                                        </form>

                                                                    </tr>

                                                                <% } %>
                                                            <% } %>

<%}%>
                                                            </tbody>

                                                        </table>
                                                    </div>


                                                    <!-- week lesson ends -->

                                                </div>
                                                <%}%>
                                                <!-- add new ends -->

                                            </div>
                                        </div>

                                        <div class="tab-pane fade" id="review" role="tabpanel" aria-labelledby="review-tab">
                                            <div class="course__review">
                                                <h3>Reviews</h3>


                                                <div class="course__review-rating mb-50">
                                                    <div class="row g-0">
                                                        <div class="col-xxl-4 col-xl-4 col-lg-4 col-md-4 col-sm-4">
                                                            <div class="course__review-rating-info grey-bg text-center">
                                                                <h5>5</h5>
                                                                <ul>
                                                                    <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                    <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                    <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                    <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                    <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                </ul>
                                                                <p>4 Ratings</p>
                                                            </div>
                                                        </div>
                                                        <div class="col-xxl-8 col-xl-8 col-lg-8 col-md-8 col-sm-8">
                                                            <div class="course__review-details grey-bg">
                                                                <h5>Detailed Rating</h5>
                                                                <div class="course__review-content mb-20">
                                                                    <div class="course__review-item d-flex align-items-center justify-content-between">
                                                                        <div class="course__review-text">
                                                                            <span>5 stars</span>
                                                                        </div>
                                                                        <div class="course__review-progress">
                                                                            <div class="single-progress" data-width="100%"></div>
                                                                        </div>
                                                                        <div class="course__review-percent">
                                                                            <h5>100%</h5>
                                                                        </div>
                                                                    </div>
                                                                    <div class="course__review-item d-flex align-items-center justify-content-between">
                                                                        <div class="course__review-text">
                                                                            <span>4 stars</span>
                                                                        </div>
                                                                        <div class="course__review-progress">
                                                                            <div class="single-progress" data-width="30%"></div>
                                                                        </div>
                                                                        <div class="course__review-percent">
                                                                            <h5>30%</h5>
                                                                        </div>
                                                                    </div>
                                                                    <div class="course__review-item d-flex align-items-center justify-content-between">
                                                                        <div class="course__review-text">
                                                                            <span>3 stars</span>
                                                                        </div>
                                                                        <div class="course__review-progress">
                                                                            <div class="single-progress" data-width="0%"></div>
                                                                        </div>
                                                                        <div class="course__review-percent">
                                                                            <h5>0%</h5>
                                                                        </div>
                                                                    </div>
                                                                    <div class="course__review-item d-flex align-items-center justify-content-between">
                                                                        <div class="course__review-text">
                                                                            <span>2 stars</span>
                                                                        </div>
                                                                        <div class="course__review-progress">
                                                                            <div class="single-progress" data-width="0%"></div>
                                                                        </div>
                                                                        <div class="course__review-percent">
                                                                            <h5>0%</h5>
                                                                        </div>
                                                                    </div>
                                                                    <div class="course__review-item d-flex align-items-center justify-content-between">
                                                                        <div class="course__review-text">
                                                                            <span>1 stars</span>
                                                                        </div>
                                                                        <div class="course__review-progress">
                                                                            <div class="single-progress" data-width="0%"></div>
                                                                        </div>
                                                                        <div class="course__review-percent">
                                                                            <h5>0%</h5>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>



                                                <div class="course__comment mb-75">
                                                    <h3><%=reviews.length%> Comments</h3>

                                                    <%for(let review of reviews){%>
                                                    <ul>
                                                        <li>
                                                            <div class="course__comment-box ">
                                                                <div class="course__comment-thumb float-start">
                                                                    <img src="/images/course/comment/course-comment-1.jpg" alt="">
                                                                </div>
                                                                <div class="course__comment-content">
                                                                    <div class="course__comment-wrapper ml-70 fix">
                                                                        <div class="course__comment-info float-start">
                                                                            <h4><%=review[2]%></h4>
                                                                            <span><%=review[3]%></span>
                                                                        </div>
                                                                        <div class="course__comment-rating float-start float-sm-end">
                                                                            <ul>
                                                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                                <li><a href="#"> <i class="icon_star"></i> </a></li>
                                                                                <li><a href="#" class="no-rating"> <i class="icon_star"></i> </a></li>
                                                                            </ul>
                                                                            <h5><%= review[1]%></h5>
                                                                        </div>
                                                                    </div>
                                                                    <div class="course__comment-text ml-70">
                                                                        <p><%=review[0]%></p>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </li>
                                                    </ul>
                                                    <% }%>
                                                </div>


                                            </div>
                                        </div>

                                        <div class="tab-pane fade" id="member" role="tabpanel" aria-labelledby="member-tab">
                                            <div class="course__member mb-45">

                                                <%for(let member of members){%>

                                                <div class="course__member-item">
                                                    <div class="row align-items-center">
                                                        <div class="col-xxl-5 col-xl-5 col-lg-5 col-md-5 col-sm-6">
                                                            <div class="course__member-thumb d-flex align-items-center">
                                                                <img src="/images/course/instructor/course-instructor-1.jpg" alt="">
                                                                <div class="course__member-name ml-20">
                                                                    <h5><%=member[0]%></h5>
                                                                    <span>Student</span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-xxl-2 col-xl-2 col-lg-2 col-md-2 col-sm-2 col-4">
                                                            <div class="course__member-info pl-45">
                                                                <h5>Mail</h5>
                                                                <span><%=member[1]%></span>
                                                            </div>
                                                        </div>


                                                    </div>
                                                </div>
                                                <% } %>


                                            </div>
                                        </div>
                                        <div class="course__share">
                                            <h3>Share :</h3>
                                            <ul>
                                                <li><a href="#" class="fb" ><i class="social_facebook"></i></a></li>
                                                <li><a href="#" class="tw" ><i class="social_twitter"></i></a></li>
                                                <li><a href="#" class="pin" ><i class="social_pinterest"></i></a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </section>
            <!-- page title area end -->

    <% } %>

</main>

<!-- footer area start -->
<footer>
    <div class="footer__area footer-bg">
        <div class="footer__top pt-90 pb-40">
            <div class="container">
                <div class="row">
                    <div class="col-xxl-3 col-xl-3 col-lg-3 col-md-4 col-sm-6">
                        <div class="footer__widget mb-50">
                            <div class="footer__widget-head mb-22">
                                <div class="footer__logo">
